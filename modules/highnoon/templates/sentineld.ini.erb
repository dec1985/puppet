[sentineld]
name = sentineld
host = <%= @hostname %>
port = 26379

[masters]
   [[masterA]]
    host = sso-redis1
    port = 36379
    level = 2
    down_after_ms = 10000
    failover_timeout = 90000
    can_failover = yes
    parallel_syncs = 1
    notification_script = redis_down_notification.py

